# Configurações do Banco de Dados PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ibge_data
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui
DB_SCHEMA=public

# Configurações da API do IBGE
IBGE_BASE_URL=https://servicodados.ibge.gov.br/api/v1
